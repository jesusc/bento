<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.gbind.resource.gbind.mopp.GbindMetaInformation" id="org.emftext.language.gbind.resource.gbind.mopp.GbindMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.gbind.resource.gbind.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.gbind.resource.gbind.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.gbind.resource.gbind.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.gbind.resource.gbind.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.gbind.resource.gbind.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.nature" name="gbind nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.gbind.resource.gbind.mopp.GbindNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.gbind.resource.gbind.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.gbind.resource.gbind.builder" name="gbind Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.gbind.resource.gbind.mopp.GbindBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.gbind.resource.gbind.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.gbind.resource.gbind.mopp.GbindResourceFactoryDelegator" type="gbind">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.gbind.resource.gbind.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="org.emftext.language.gbind.resource.gbind.debug.GbindLineBreakpoint" id="org.emftext.language.gbind.resource.gbind.debug.breakpoint" markerType="org.emftext.language.gbind.resource.gbind.debug.lineBreakpoint.marker" name="gbind Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="org.emftext.language.gbind.resource.gbind.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
		<launchConfigurationType id="org.emftext.language.gbind.resource.gbind.ui.launchConfigurationType" delegate="org.emftext.language.gbind.resource.gbind.launch.GbindLaunchConfigurationDelegate" modes="run,debug" name="gbind Application" sourceLocatorId="org.emftext.language.gbind.resource.gbind.ui.sourceLocator" sourcePathComputerId="org.emftext.language.gbind.resource.gbind.ui.sourcePathComputer" delegateName="gbind Launch Tooling" delegateDescription="This will run or debug .gbind files.">
		</launchConfigurationType>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourcePathComputers">
		<sourcePathComputer class="org.emftext.language.gbind.resource.gbind.debug.GbindSourcePathComputerDelegate" id="org.emftext.language.gbind.resource.gbind.ui.sourcePathComputer">
		</sourcePathComputer>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourceLocators">
		<sourceLocator id="org.emftext.language.gbind.resource.gbind.ui.sourceLocator" class="org.emftext.language.gbind.resource.gbind.debug.GbindSourceLocator" name="gbind Source Locator">
		</sourceLocator>
	</extension>
	
</plugin>
