composite component bpmn2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"  
	binding bindBPMN = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2pn/bpmn2fd.gbind"
			
	source concept BPMN  : "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2pn/bpmn.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : BPMN  = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2pn/example.bpmn" 
	target model out : PN    = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi" 
	
	compose apply fd2pn(bindBPMN(in)) -> (out)
}