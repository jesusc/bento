	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
		<launchConfigurationTypeImage icon="/icons/launch_type_icon.gif" configTypeID="genericity.language.gcomponent.resource.gcomponent.ui.launchConfigurationType" id="genericity.language.gcomponent.launchImage">
		</launchConfigurationTypeImage>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
		<launchConfigurationTabGroup type="genericity.language.gcomponent.resource.gcomponent.ui.launchConfigurationType" class="genericity.language.gcomponent.resource.gcomponent.ui.launch.GcomponentLaunchConfigurationTabGroup" id="genericity.language.gcomponent.launchConfigurationTabGroup" helpContextId="genericity.language.gcomponent.launchConfigHelpContext">
			<launchMode mode="debug" perspective="org.eclipse.debug.ui.DebugPerspective" description="Debug gcomponent model.">
			</launchMode>
			<launchMode mode="run" description="Run gcomponent model.">
			</launchMode>
		</launchConfigurationTabGroup>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchShortcuts">
		<shortcut label="gcomponent Application" icon="/icons/launch_shortcut_icon.gif" helpContextId="genericity.language.gcomponent.launch" modes="run,debug" class="genericity.language.gcomponent.resource.gcomponent.ui.launch.GcomponentLaunchShortcut" description="Launch a gcomponent model" id="genericity.language.gcomponent.launchShortcut">
			<description description="Run gcomponent model" mode="run">
			</description>
			<description description="Debug gcomponent model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="genericity.language.gcomponent.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
			<configurationType id="genericity.language.gcomponent.resource.gcomponent.ui.launchConfigurationType">
			</configurationType>
		</shortcut>
	</extension>
	
	<extension point="org.eclipse.core.expressions.propertyTesters">
		<propertyTester id="genericity.language.gcomponent.PropertyTester" type="java.lang.Object" namespace="genericity.language.gcomponent" properties="isLaunchable" class="genericity.language.gcomponent.resource.gcomponent.ui.GcomponentPropertyTester">
		</propertyTester>
	</extension>
	
	<extension point="org.eclipse.ui.editors.annotationTypes">
