			</adapter>
		</factory>
		<factory adaptableType="genericity.language.gcomponent.resource.gcomponent.debug.GcomponentDebugVariable" class="genericity.language.gcomponent.resource.gcomponent.ui.debug.GcomponentAdapterFactory">
			<adapter type="org.eclipse.debug.internal.ui.viewers.model.provisional.IElementLabelProvider">
