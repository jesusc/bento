		</wizard>
	</extension>
	
	<extension point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator class="genericity.language.gcomponent.resource.gcomponent.ui.GcomponentMarkerResolutionGenerator" markerType="genericity.language.gcomponent.resource.gcomponent.problem">
		</markerResolutionGenerator>
	</extension>
	
	<extension point="org.eclipse.core.filebuffers.annotationModelCreation">
		<factory class="genericity.language.gcomponent.resource.gcomponent.ui.GcomponentAnnotationModelFactory" extensions="gcomponent">
		</factory>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="genericity.language.gcomponent.resource.gcomponent" name=".gcomponent File" base-type="org.eclipse.core.runtime.text" file-extensions="gcomponent">
		</content-type>
	</extension>
	
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider class="org.eclipse.ui.editors.text.TextFileDocumentProvider" extensions="gcomponent" id="genericity.language.gcomponent.resource.gcomponent.ui.provider">
		</provider>
	</extension>
	
	<extension point="org.eclipse.ui.editors.annotationTypes">
		<type name="genericity.language.gcomponent.resource.gcomponent.ui.occurences">
		</type>
		<type name="genericity.language.gcomponent.resource.gcomponent.ui.occurences.declaration" super="genericity.language.gcomponent.resource.gcomponent.ui.occurences">
		</type>
	</extension>
	
	<extension point="org.eclipse.ui.editors.markerAnnotationSpecification">
