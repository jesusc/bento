module java2uml;
create OUT : Concrete from IN : UML;

rule bind_Class_UMLClass {
	from j : UML!Class
	to   c : Concrete!UMLClass
}

helper context UML!Class def: name : String = self.umlName;

helper context UML!Class def: superclasses : Set(Concrete!UMLClass) = 
	self.generalizations->collect(g | g.reference);

