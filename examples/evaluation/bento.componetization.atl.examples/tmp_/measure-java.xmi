<?xml version="1.0" encoding="ISO-8859-1"?>
<RootMeasureSet xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns="Measure" modelType="UML2">
  <categories name="MOOD" desc="Metrics for Object-Oriented Design">
    <metrics name="AIF" desc="Attributes Inheritance Factor" preferredValue="!=0"/>
    <metrics name="OIF" desc="Operations Inheritance Factor" preferredValue="!=0"/>
    <metrics name="AHF" desc="Attributes Hiding Factor" preferredValue="!=0"/>
    <metrics name="OHF" desc="Operations Hiding Factor" preferredValue="!=0"/>
    <metrics name="BPF" desc="Behavioral Polymorphism Factor" preferredValue="!=0"/>
    <metrics name="CCF" desc="Class Coupling Factor" preferredValue="!=0"/>
    <metrics name="ICF" desc="Internal Coupling Factor" preferredValue="!=0"/>
  </categories>
  <categories name="MOOD2" desc="Metrics for Object-Oriented Design 2">
    <metrics name="IIF" desc="Internal Inheritance Factor" preferredValue="!=0"/>
    <metrics name="AHEF" desc="Attributes Hiding Effectiveness Factor" preferredValue="!=0"/>
    <metrics name="OHEF" desc="Operations Hiding Effectiveness Factor" preferredValue="!=0"/>
  </categories>
  <categories name="MOOSE" desc="Metrics for Object-Oriented Software Engineering">
    <metrics name="DIT" desc="Depth of Inheritance Tree" preferredValue="!=0"/>
    <metrics name="NOC" desc="Number of Children" preferredValue="!=0"/>
    <metrics name="CBO" desc="Coupling Between Objects" preferredValue="!=0"/>
    <metrics name="RFC" desc="Response for a Class" preferredValue="!=0"/>
  </categories>
  <categories name="EMOOSE" desc="Extended MOOSE">
    <metrics name="SIZE2" desc="Number of local Attributes and Operations defined in the Class" preferredValue="!=0"/>
  </categories>
  <categories name="QMOOD" desc="Quality Model for Object-Oriented Design">
    <metrics name="DSC" desc="Design Size in Classes" preferredValue="!=0"/>
    <metrics name="NOH" desc="Number of Hierarchies" preferredValue="!=0"/>
    <metrics name="NIC" desc="Number of Independent Classes" preferredValue="!=0"/>
    <metrics name="NSI" desc="Number of Single Inheritance" preferredValue="!=0"/>
    <metrics name="NNC" desc="Number of Internal Classes" preferredValue="!=0"/>
    <metrics name="NAC" desc="Number of Abstract Classes" preferredValue="!=0"/>
    <metrics name="NLC" desc="Number of Leaf Classes" preferredValue="!=0"/>
    <metrics name="ADI" desc="Average Depth of Inheritance" preferredValue="!=0"/>
    <metrics name="AWI" desc="Average Width of Inheritance" preferredValue="!=0"/>
    <metrics name="ANA" desc="Average Number of Ancestors" preferredValue="!=0"/>
    <metrics name="MFA" desc="Measure of Functional Abstraction" preferredValue="!=0"/>
    <metrics name="MAA" desc="Measure of Attribute Abstraction" preferredValue="!=0"/>
    <metrics name="MAT" desc="Measure of Abstraction" preferredValue="!=0"/>
    <metrics name="MOA" desc="Measure of Aggregation" preferredValue="!=0"/>
    <metrics name="MRM" desc="Modeled Relationship Measure" preferredValue="!=0"/>
    <metrics name="DAM" desc="Data Access Metric" preferredValue=">75"/>
    <metrics name="OAM" desc="Operation Access Metric" preferredValue=">75"/>
    <metrics name="MAM" desc="Member Access Metric" preferredValue=">75"/>
    <metrics name="NOA" desc="Number of Ancestors" preferredValue="!=0"/>
    <metrics name="NOM" desc="Number of Methods" preferredValue="!=0"/>
    <metrics name="CIS" desc="Class Interface Size" preferredValue="!=0"/>
    <metrics name="NPT" desc="Number of Unique Parameter Types" preferredValue="!=0"/>
    <metrics name="NPM" desc="Number of Parameters per Method" preferredValue="!=0"/>
    <metrics name="NOD" desc="Number of Attributes" preferredValue="!=0"/>
    <metrics name="NAD" desc="Number of Abstract Data Types" preferredValue="!=0"/>
    <metrics name="NPA" desc="Number of Public Attributes" preferredValue="!=0"/>
    <metrics name="CSM" desc="Class Size Metric" preferredValue="!=0"/>
    <metrics name="CAM" desc="Cohesion Among Methods of Class" preferredValue="!=0"/>
    <metrics name="DCC" desc="Direct Class Coupling" preferredValue="!=0"/>
    <metrics name="MCC" desc="Maximum Class Coupling" preferredValue="!=0"/>
    <metrics name="DAC" desc="Direct Attribute Based Coupling" preferredValue="!=0"/>
    <metrics name="DPC" desc="Direct Parameter Based Coupling" preferredValue="!=0"/>
    <metrics name="MPC" desc="Maximum Parameter Based Coupling" preferredValue="!=0"/>
    <metrics name="CCD" desc="Class Complexity Based on Data" preferredValue="!=0"/>
    <metrics name="CCP" desc="Class Complexity Based on Method Parameters" preferredValue="!=0"/>
    <metrics name="CCM" desc="Class Complexity Based on Members" preferredValue="!=0"/>
  </categories>
</RootMeasureSet>
