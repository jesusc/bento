composite component bpmn2pn_omg {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"  
	binding bindBPMN = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2.omg2pn//bpmn2.gbind"
			
	source concept BPMN  : "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2.omg2pn/bpmn.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : BPMN  = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bpmn2.omg2pn/examples/Simple1.xmi" 
	target model out : PN    = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi" 
	
	compose apply fd2pn(bindBPMN(in)) -> (out)
}