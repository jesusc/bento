@namespace(uri="http://bento/analysis/abcd", prefix="abcd")
package abcd;

abstract class NamedElt {
  attr String[1] name;
}

class Model extends NamedElt {
  val A[*] elements;
  val Other[*] others;
  attr StyleKind style;
}

class A extends NamedElt {
}

class B extends A {
  attr String propOfB;
}

class C extends A {
  attr String propOfC;
}

class C1 extends C {
}

class C2 extends C {
}

class D extends A {
  attr String propOfD;
}

class D1 extends D {
}

class D2 extends D {
}

class D3 extends D {
}

class Other extends NamedElt {
}

enum StyleKind {
  Style1 = 0;
  Style2 = 1;
}

