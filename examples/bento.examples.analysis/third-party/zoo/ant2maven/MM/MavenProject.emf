

@namespace(uri="zoo/mavenproject", prefix="zoo_mavenproject")
package MavenProject;

  class Project {
    !ordered attr String ~id;
    !ordered attr String groupId;
    !ordered attr String artifactId;
    !ordered attr String name;
    !ordered attr String description;
    !ordered val Build build;
    !ordered ref Project[*] dependencies;
  }

  class Build {
    !ordered attr String[1] defaultGoal;
    !ordered attr String[1] sourceDirectory;
  }


