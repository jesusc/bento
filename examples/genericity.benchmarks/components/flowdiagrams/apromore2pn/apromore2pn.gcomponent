composite component apromore2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"    
	binding bindAPRO = "platform:/resource/genericity.benchmarks/components/flowdiagrams/apromore2pn/apromore2fd.gbind"
			
	source concept EPC : "platform:/resource/genericity.benchmarks/components/flowdiagrams/apromore2pn/apromore.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : EPC = "platform:/resource/genericity.benchmarks/components/flowdiagrams/apromore2pn/examples/example-apro.model" 
	target model out : PN = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi"
	
	compose apply fd2pn(bindAPRO(in)) -> (out)
}