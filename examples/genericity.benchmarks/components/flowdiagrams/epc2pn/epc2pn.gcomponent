composite component epc2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"    
	binding bindEPC = "platform:/resource/genericity.benchmarks/components/flowdiagrams/epc2pn/epc.gbind"
			
	source concept EPC : "platform:/resource/genericity.benchmarks/components/flowdiagrams/epc2pn/epc.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : EPC = "platform:/resource/genericity.benchmarks/components/flowdiagrams/epc2pn/examples/example-epc.model" 
	target model out : PN = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi"
	
	compose apply fd2pn(bindEPC(in)) -> (out)
}