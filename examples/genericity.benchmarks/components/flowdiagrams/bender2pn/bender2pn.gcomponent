composite component bender2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"  
	binding bindBENDER = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bender2pn/bender.gbind"
			
	source concept BENDER  : "platform:/resource/genericity.benchmarks/components/flowdiagrams/bender2pn/StateMachine.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : BENDER  = "platform:/resource/genericity.benchmarks/components/flowdiagrams/bender2pn/examples/helloworld.diagram" 
	target model out : PN    = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi" 
	
	compose apply fd2pn(bindBENDER(in)) -> (out)
}