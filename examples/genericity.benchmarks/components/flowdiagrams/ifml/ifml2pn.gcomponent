composite component ifml2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"  
	binding bindIFML = "platform:/resource/genericity.benchmarks/components/flowdiagrams/ifml/ifml2fd.gbind"
			
	source concept IFML  : "platform:/resource/genericity.benchmarks/components/flowdiagrams/ifml/IFML.ecore"
	target concept PN    : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : IFML  = "platform:/resource/genericity.benchmarks/components/flowdiagrams/ifml/example.ifml" 
	target model out : PN    = "platform:/resource/genericity.benchmarks/components/flowdiagrams/models/output_test.xmi" 
	
	compose apply fd2pn(bindIFML(in)) -> (out)
}