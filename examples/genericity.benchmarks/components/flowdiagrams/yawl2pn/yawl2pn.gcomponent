composite component yawl2pn {
	uses "platform:/resource/genericity.benchmarks/components/flowdiagrams/fd2pn.gcomponent"    
	binding bindYAWL = "platform:/resource/genericity.benchmarks/components/flowdiagrams/yawl2pn/yawl.gbind"
			
	source concept YAWL : "platform:/resource/genericity.benchmarks/components/flowdiagrams/yawl2pn/yawl.ecore"
	target concept PN   : "platform:/resource/genericity.benchmarks/components/flowdiagrams/metamodels/petri_nets.ecore"
	source model in  : YAWL = "platform:/resource/genericity.benchmarks/components/flowdiagrams/yawl2pn/examples/yawl_ex1.model" 
	target model out : PN   = "platform:/resource/genericity.benchmarks/components/flowdiagrams/yawl2pn/examples/pn.xmi"
	
	compose apply fd2pn(bindYAWL(in)) -> (out)
}