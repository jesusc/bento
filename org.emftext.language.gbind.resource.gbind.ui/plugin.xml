			</launchMode>
			<launchMode mode="run" description="Run gbind model.">
			</launchMode>
		</launchConfigurationTabGroup>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchShortcuts">
		<shortcut label="gbind Application" icon="/icons/launch_shortcut_icon.gif" helpContextId="org.emftext.language.gbind.launch" modes="run,debug" class="org.emftext.language.gbind.resource.gbind.ui.launch.GbindLaunchShortcut" description="Launch a gbind model" id="org.emftext.language.gbind.launchShortcut">
			<description description="Run gbind model" mode="run">
			</description>
			<description description="Debug gbind model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="org.emftext.language.gbind.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
			<configurationType id="org.emftext.language.gbind.resource.gbind.ui.launchConfigurationType">
			</configurationType>
		</shortcut>
	</extension>
	
	<extension point="org.eclipse.core.expressions.propertyTesters">
		<propertyTester id="org.emftext.language.gbind.PropertyTester" type="java.lang.Object" namespace="org.emftext.language.gbind" properties="isLaunchable" class="org.emftext.language.gbind.resource.gbind.ui.GbindPropertyTester">
		</propertyTester>
	</extension>
	
