		</launchConfigurationTypeImage>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
		<launchConfigurationTabGroup type="org.emftext.language.gbind.resource.gbind.ui.launchConfigurationType" class="org.emftext.language.gbind.resource.gbind.ui.launch.GbindLaunchConfigurationTabGroup" id="org.emftext.language.gbind.launchConfigurationTabGroup" helpContextId="org.emftext.language.gbind.launchConfigHelpContext">
